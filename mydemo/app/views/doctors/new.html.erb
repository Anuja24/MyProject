<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Download Link</th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    <% @doctors.each do |d| %>
      <tr>
        <td><%= d.name %></td>
        <td><%= link_to "Download doc", d.avatar.url %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>